  </owl:Ontology>
  <!-- Classes -->
  <!-- Class: Base carrier category -->
  <rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/C10009">
    <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/" />
    <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
    <rdfs:subClassOf rdf:resource="http://rdaregistry.info/Elements/rof/C10003" />
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class" />
    <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/BaseCarrierCategory.en" />
    <rdfs:subClassOf rdf:resource="http://rdaregistry.info/Elements/rof/C10004" />
    <rdfs:label xml:lang="de">Basis-Datenträger-Kategorie</rdfs:label>
    <skos:definition xml:lang="de">Eine Basis-Kategorie, die nur durch ein oder mehrere Merkmale des Datenträgers einer Ressource definiert ist.</skos:definition>
  </rdf:Description>
  <!-- Class: Base category -->
  <rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/C10003">
    <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/" />
    <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
    <rdfs:subClassOf rdf:resource="http://rdaregistry.info/Elements/rof/C10006" />
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class" />
    <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/BaseCategory.en" />
    <reg:hasSubClass rdf:resource="http://rdaregistry.info/Elements/rof/C10008" />
    <reg:hasSubClass rdf:resource="http://rdaregistry.info/Elements/rof/C10009" />
    <rdfs:label xml:lang="de">Basis-Kategorie</rdfs:label>
    <skos:definition xml:lang="de">Eine Kategorie für eine Ressource, die vollständig durch Primärwerte definiert ist, die im RDA/ONIX-Framework angegeben sind.</skos:definition>
  </rdf:Description>
  <!-- Class: Base content category -->
  <rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/C10008">
    <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/" />
    <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
    <rdfs:subClassOf rdf:resource="http://rdaregistry.info/Elements/rof/C10003" />
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class" />
    <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/BaseContentCategory.en" />
    <rdfs:subClassOf rdf:resource="http://rdaregistry.info/Elements/rof/C10005" />
    <rdfs:label xml:lang="de">Basis-Inhalts-Kategorie</rdfs:label>
    <skos:definition xml:lang="de">Eine Basis-Kategorie, die nur durch ein oder mehrere Merkmale des Inhalts einer Ressource definiert ist.</skos:definition>
  </rdf:Description>
  <!-- Class: Carrier category -->
  <rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/C10004">
    <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/" />
    <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
    <rdfs:subClassOf rdf:resource="http://rdaregistry.info/Elements/rof/C10006" />
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class" />
    <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/CarrierCategory.en" />
    <reg:hasSubClass rdf:resource="http://rdaregistry.info/Elements/rof/C10002" />
    <reg:hasSubClass rdf:resource="http://rdaregistry.info/Elements/rof/C10009" />
    <rdfs:label xml:lang="de">Datenträger-Kategorie</rdfs:label>
    <skos:definition xml:lang="de">Eine Klasse von Ressourcen, die durch bestimmte Werte für ein Merkmal oder mehrere Merkmale der Art und Weise, mit denen Inhalte übermittelt werden, definiert wird.</skos:definition>
  </rdf:Description>
  <!-- Class: Category -->
  <rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/C10006">
    <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/" />
    <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class" />
    <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/Category.en" />
    <reg:hasSubClass rdf:resource="http://rdaregistry.info/Elements/rof/C10003" />
    <reg:hasSubClass rdf:resource="http://rdaregistry.info/Elements/rof/C10004" />
    <reg:hasSubClass rdf:resource="http://rdaregistry.info/Elements/rof/C10005" />
    <reg:hasSubClass rdf:resource="http://rdaregistry.info/Elements/rof/C10007" />
    <rdfs:label xml:lang="de">Kategorie</rdfs:label>
    <skos:definition xml:lang="de">Eine Klasse von Ressourcen, die durch bestimmte Werte für ein Merkmal oder mehrere Merkmale (zum Beispiel alle Ressourcen, bei denen der Wert für die Eigenschaft Art Sprache ist) definiert wird.</skos:definition>
  </rdf:Description>
  <!-- Class: Content category -->
  <rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/C10005">
    <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/" />
    <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
    <rdfs:subClassOf rdf:resource="http://rdaregistry.info/Elements/rof/C10006" />
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class" />
    <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/ContentCategory.en" />
    <reg:hasSubClass rdf:resource="http://rdaregistry.info/Elements/rof/C10001" />
    <reg:hasSubClass rdf:resource="http://rdaregistry.info/Elements/rof/C10008" />
    <rdfs:label xml:lang="de">Inhalts-Kategorie</rdfs:label>
    <skos:definition xml:lang="de">Eine Klasse von Ressourcen, die durch bestimmte Werte für ein Merkmal oder mehrere Merkmale des geistigen oder künstlerischen Inhalts einer Ressource definiert wird.</skos:definition>
  </rdf:Description>
  <!-- Class: Qualified carrier category -->
  <rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/C10002">
    <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/" />
    <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
    <rdfs:subClassOf rdf:resource="http://rdaregistry.info/Elements/rof/C10007" />
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class" />
    <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/QualifiedCarrierCategory.en" />
    <rdfs:subClassOf rdf:resource="http://rdaregistry.info/Elements/rof/C10004" />
    <rdfs:label xml:lang="de">Zugelassene Datenträger-Kategorie</rdfs:label>
    <skos:definition xml:lang="de">Eine Kategorie für eine Ressource, die eine Basis-Datenträger-Kategorie zulässt.</skos:definition>
  </rdf:Description>
  <!-- Class: Qualified category -->
  <rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/C10007">
    <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/" />
    <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
    <rdfs:subClassOf rdf:resource="http://rdaregistry.info/Elements/rof/C10006" />
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class" />
    <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/QualifiedCategory.en" />
    <reg:hasSubClass rdf:resource="http://rdaregistry.info/Elements/rof/C10001" />
    <reg:hasSubClass rdf:resource="http://rdaregistry.info/Elements/rof/C10002" />
    <rdfs:label xml:lang="de">Zugelassene Kategorie</rdfs:label>
    <skos:definition xml:lang="de">Eine Kategorie, die eine Basis-Kategorie zulässt, indem ein Wert oder mehrere Werte ergänzt werden, die nicht im RDA/Onix Framework bestimmt sind.</skos:definition>
  </rdf:Description>
  <!-- Class: Qualified content category -->
  <rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/C10001">
    <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/" />
    <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
    <rdfs:subClassOf rdf:resource="http://rdaregistry.info/Elements/rof/C10007" />
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class" />
    <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/QualifiedContentCategory.en" />
    <rdfs:subClassOf rdf:resource="http://rdaregistry.info/Elements/rof/C10005" />
    <rdfs:label xml:lang="de">Qualifizierte Inhalts-Kategorie</rdfs:label>
    <skos:definition xml:lang="de">Eine Kategorie für eine Ressource, die eine Basis-Inhalts-Kategorie zulässt.</skos:definition>
  </rdf:Description>
  <!-- Properties -->
  <!-- Property: has applied material -->
  <rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10001">
    <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/" />
    <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
    <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasAppliedMaterial.en" />
    <rdfs:label xml:lang="de">hat aufgebrachtes Material</rdfs:label>
    <skos:definition xml:lang="de">Ein Material, das auf das Trägermaterial aufgebracht wird, um den Inhalt der Ressource einzufügen.</skos:definition>
    <skos:scopeNote xml:lang="de">Dies trifft auf Ressourcen zu, die Inhalte auf einem physischen Datenträger vermitteln. Für Ressourcen, die Inhalte durch Hörfunk- oder Online-Übertragung vermitteln, wird der Wert nicht anwendbar vergeben.</skos:scopeNote>
  </rdf:Description>
  <!-- Property: has base material -->
  <rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10007">
    <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/" />
    <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
    <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasBaseMaterial.en" />
    <rdfs:label xml:lang="de">hat Trägermaterial</rdfs:label>
    <skos:definition xml:lang="de">Ein zugrunde liegendes physisches Material, auf oder in dem der Inhalt einer Ressource gespeichert ist.</skos:definition>
    <skos:scopeNote xml:lang="de">Dies trifft auf Ressourcen zu, die Inhalte auf einem physischen Datenträger vermitteln. Für Ressourcen, die Inhalte durch Hörfunk- oder Online-Übertragung vermitteln, wird der Wert nicht anwendbar vergeben.</skos:scopeNote>
  </rdf:Description>
  <!-- Property: has capture method -->
  <rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10017">
    <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/" />
    <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
    <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasCaptureMethod.en" />
    <rdfs:label xml:lang="de">hat Aufzeichnungsmethode</rdfs:label>
    <skos:definition xml:lang="de">Ein Prozess, der zur Erfassung des Inhalts einer Ressource verwendet wird.</skos:definition>
    <skos:scopeNote xml:lang="de">Die Aufzeichnungsmethode ist die Methode, die bei der anfänglichen Erfassung des Inhalts verwendet wird, nicht die Methode, mit der der Inhalt auf dem Träger fixiert wird (zum Beispiel Befestigungsmethode).</skos:scopeNote>
  </rdf:Description>
  <!-- Property: has character -->
  <rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10012">
    <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/" />
    <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
    <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasCharacter.en" />
    <rdfs:label xml:lang="de">hat Art</rdfs:label>
    <skos:definition xml:lang="de">Eine grundlegende Form der Übertragung, in der der Inhalt einer Ressource ausgedrückt wird.</skos:definition>
  </rdf:Description>
  <!-- Property: has encoding format -->
  <rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10004">
    <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/" />
    <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
    <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasEncodingFormat.en" />
    <rdfs:label xml:lang="de">hat Kodierungsformat</rdfs:label>
    <skos:definition xml:lang="de">Ein Schema, Standard und so weiter, das/der zum Kodieren des digitalen Inhalts einer Ressource verwendet wird.</skos:definition>
  </rdf:Description>
  <!-- Property: has extension mode -->
  <rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10015">
    <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/" />
    <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
    <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasExtensionMode.en" />
    <rdfs:label xml:lang="de">hat Erweiterungsform</rdfs:label>
    <skos:definition xml:lang="de">Eine Angabe, wie der Inhalt der Ressource nach der Erstveröffentlichung voraussichtlich erweitert wird.</skos:definition>
  </rdf:Description>
  <!-- Property: has extension requirement -->
  <rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10020">
    <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/" />
    <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
    <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasExtensionRequirement.en" />
    <rdfs:label xml:lang="de">hat Erweiterungsanforderung</rdfs:label>
    <skos:definition xml:lang="de">Eine Festlegung, dass eine Erweiterung des Inhalts der Ressource nach der Erstveröffentlichung notwendig sein wird, um sie zu vervollständigen.</skos:definition>
  </rdf:Description>
  <!-- Property: has extension termination -->
  <rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10022">
    <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/" />
    <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
    <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasExtensionTermination.en" />
    <rdfs:label xml:lang="de">hat Erweiterungsabschluss</rdfs:label>
    <skos:definition xml:lang="de">Eine Einschätzung über die Dauer der Erweiterung des Inhalts der Ressource nach der Erstveröffentlichung.</skos:definition>
  </rdf:Description>
  <!-- Property: has fixation method -->
  <rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10003">
    <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/" />
    <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
    <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasFixationMethod.en" />
    <rdfs:label xml:lang="de">hat Befestigungsmethode</rdfs:label>
    <skos:definition xml:lang="de">Eine Methode oder ein Prozess, durch den der Inhalt einer Ressource in den Datenträger eingefügt oder eingebettet wird.</skos:definition>
  </rdf:Description>
  <!-- Property: has fixation tool -->
  <rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10006">
    <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/" />
    <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
    <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasFixationTool.en" />
    <rdfs:label xml:lang="de">hat Befestigungsgerät</rdfs:label>
    <skos:definition xml:lang="de">Ein Werkzeug, mit dem der Inhalt einer Ressource ergänzt werden kann.</skos:definition>
  </rdf:Description>
  <!-- Property: has form or genre -->
  <rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10014">
    <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/" />
    <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
    <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasFormOrGenre.en" />
    <rdfs:label xml:lang="de">hat Form oder Genre</rdfs:label>
    <skos:definition xml:lang="de">Eine literarische, musikalische, künstlerische und so weiter Form oder ein literarisches, musikalisches, künstlerisches und so weiter Genre, die/das mit dem Inhalt einer Ressource in Verbindung steht.</skos:definition>
  </rdf:Description>
  <!-- Property: has housing format -->
  <rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10009">
    <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/" />
    <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
    <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasHousingFormat.en" />
    <rdfs:label xml:lang="de">hat Format des Gehäuses</rdfs:label>
    <skos:definition xml:lang="de">Die physische Form des Gehäuses für das Speichermedium.</skos:definition>
    <skos:scopeNote xml:lang="de">Dies trifft auf Ressourcen zu, die Inhalte auf einem physischen Datenträger vermitteln. Für Ressourcen, die Inhalte durch Hörfunk- oder Online-Übertragung vermitteln, wird der Wert nicht anwendbar vergeben.</skos:scopeNote>
  </rdf:Description>
  <!-- Property: has image dimensionality -->
  <rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10024">
    <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/" />
    <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
    <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasImageDimensionality.en" />
    <rdfs:label xml:lang="de">hat Dimensionalität eines Bilds</rdfs:label>
    <skos:definition xml:lang="de">Die Anzahl der räumlichen Dimensionen, in denen der Bildinhalt einer Ressource wahrgenommen werden soll.</skos:definition>
    <skos:scopeNote xml:lang="de">Dies trifft nur auf Inhalte zu, für die als Art der Wert Bild vergeben wurde.</skos:scopeNote>
  </rdf:Description>
  <!-- Property: has image movement -->
  <rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10025">
    <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/" />
    <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
    <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasImageMovement.en" />
    <rdfs:label xml:lang="de">hat Bildbewegung</rdfs:label>
    <skos:definition xml:lang="de">Das wahrgenommene Vorhandensein beziehungsweise nicht Vorhandensein von Bewegung im Bildinhalt einer Ressource.</skos:definition>
    <skos:scopeNote xml:lang="de">Dies trifft nur auf Inhalte zu, für die als Art der Wert Bild vergeben wurde.</skos:scopeNote>
  </rdf:Description>
  <!-- Property: has interaction -->
  <rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10011">
    <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/" />
    <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
    <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasInteraction.en" />
    <rdfs:label xml:lang="de">hat Interaktion</rdfs:label>
    <skos:definition xml:lang="de">Eine Funktion, auf Aktionen des Benutzenden in Bezug auf den Inhalt der Ressource zu reagieren.</skos:definition>
  </rdf:Description>
  <!-- Property: has intermediation method -->
  <rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10002">
    <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/" />
    <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
    <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasIntermediationMethod.en" />
    <rdfs:label xml:lang="de">hat Vermittlungsmethode</rdfs:label>
    <skos:definition xml:lang="de">Eine Methode, die auf den Datenträger angewendet werden soll, damit der Inhalt der Ressource wahrgenommen werden kann.</skos:definition>
  </rdf:Description>
  <!-- Property: has intermediation tool -->
  <rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10005">
    <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/" />
    <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
    <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasIntermediationTool.en" />
    <rdfs:label xml:lang="de">hat Vermittlungsgerät</rdfs:label>
    <skos:definition xml:lang="de">Ein Hilfsmittel, mit dem der Datenträger angewendet werden soll, damit der Inhalt der Ressource wahrgenommen werden kann.</skos:definition>
    <skos:scopeNote xml:lang="de">Das Vermittlungsgerät bezeichnet jedes physische oder digitale Hilfsmittel, das erforderlich ist, um den Inhalt der Ressource bestimmungsgemäß wahrnehmen zu können, einschließlich der Werkzeuge, die zur Entschlüsselung, Dekodierung oder sonstigen Umgehung von Sicherheitsmaßnahmen erforderlich sind.</skos:scopeNote>
  </rdf:Description>
  <!-- Property: has purpose -->
  <rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10019">
    <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/" />
    <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
    <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasPurpose.en" />
    <rdfs:label xml:lang="de">hat Ziel</rdfs:label>
    <skos:definition xml:lang="de">Eine Verwendung, für die der Inhalt einer Ressource bestimmt ist.</skos:definition>
  </rdf:Description>
  <!-- Property: has revision mode -->
  <rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10016">
    <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/" />
    <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
    <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasRevisionMode.en" />
    <rdfs:label xml:lang="de">hat Überarbeitungsmodus</rdfs:label>
    <skos:definition xml:lang="de">Eine Angabe, wie der Inhalt der Ressource nach der Erstveröffentlichung voraussichtlich überarbeitet wird.</skos:definition>
  </rdf:Description>
  <!-- Property: has revision requirement -->
  <rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10021">
    <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/" />
    <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
    <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasRevisionRequirement.en" />
    <rdfs:label xml:lang="de">hat Überarbeitungsanforderung</rdfs:label>
    <skos:definition xml:lang="de">Eine Festlegung, dass eine Überarbeitung des Inhalts der Ressource nach der Erstveröffentlichung notwendig sein wird, um sie zu vervollständigen.</skos:definition>
  </rdf:Description>
  <!-- Property: has revision termination -->
  <rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10023">
    <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/" />
    <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
    <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasRevisionTermination.en" />
    <rdfs:label xml:lang="de">hat Überarbeitungsabschluss</rdfs:label>
    <skos:definition xml:lang="de">Eine Einschätzung über die Dauer der Überarbeitung des Inhalts der Ressource nach der Erstveröffentlichung.</skos:definition>
  </rdf:Description>
  <!-- Property: has sensory mode -->
  <rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10013">
    <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/" />
    <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
    <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasSensoryMode.en" />
    <rdfs:label xml:lang="de">hat sensorische Form</rdfs:label>
    <skos:definition xml:lang="de">Ein menschlicher Sinn, durch den der Inhalt einer Ressource wahrgenommen werden soll.</skos:definition>
    <skos:scopeNote xml:lang="de">Dieses Merkmal bezieht sich auf einen menschlichen Sinn, mit dem der Inhalt einer Ressource wahrgenommen werden soll. Er unterscheidet sich zu einem Sinn, mit dem der Inhalt entweder zufällig oder mit Hilfe eines anderen Hilfsmittels, als dem normalerweise zu erwartenden, wahrgenommen werden könnte (zum Beispiel ein Gerät zur Umwandlung digital kodierter Buchstaben in eine simulierte Stimme).</skos:scopeNote>
  </rdf:Description>
  <!-- Property: has storage medium format -->
  <rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10008">
    <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/" />
    <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
    <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasStorageMediumFormat.en" />
    <rdfs:label xml:lang="de">hat Format des Speichermediums</rdfs:label>
    <skos:definition xml:lang="de">Die physische Form des Materials, auf dem der Inhalt der Ressource gespeichert ist.</skos:definition>
  </rdf:Description>
  <!-- Property: has subject -->
  <rdf:Description rdf:about="http://rdaregistry.info/Elements/rof/P10018">
    <rdfs:isDefinedBy rdf:resource="http://rdaregistry.info/Elements/rof/" />
    <reg:status rdf:resource="http://metadataregistry.org/uri/RegStatus/1001" />
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property" />
    <reg:lexicalAlias rdf:resource="http://rdaregistry.info/Elements/rof/hasSubject.en" />
    <rdfs:label xml:lang="de">hat Thema</rdfs:label>
    <skos:definition xml:lang="de">Ein Thema, eine Fachrichtung, ein Geografikum, ein Ereignis, eine Person und so weiter, das/die mit dem Inhalt einer Ressource in Verbindung steht; also das, worum es im Inhalt geht.</skos:definition>
  </rdf:Description>
  <!-- Status properties used in this document  -->
  <skos:Concept rdf:about="http://metadataregistry.org/uri/RegStatus/1001" />
</rdf:RDF>